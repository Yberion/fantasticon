{
  "name": "fantasticon",
  "version": "0.0.0",
  "description": "Icon font generation tool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tancredi/fantasticon.git"
  },
  "keywords": [
    "icons",
    "fonts",
    "iconfonts",
    "svg",
    "vector"
  ],
  "author": "Tancredi Trugenberger",
  "bugs": {
    "url": "https://github.com/tancredi/fantasticon/issues"
  },
  "homepage": "https://github.com/tancredi/fantasticon#readme",
  "main": "dist/index.js",
  "bin": {
    "fantasticon": "bin/fantasticon"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build:tsc": "rimraf lib && tsc --project ./tsconfig.json",
    "build": "tsup",
    "watch": "npm run build -- --watch",
    "typecheck": "tsc --noEmit --project tsconfig.test.json --noEmit",
    "lint": "prettier --check .",
    "fix": "prettier --write .",
    "format": "prettier --write ."
  },
  "files": [
    "bin/fantasticon",
    "dist/**/*.{js,ts,map}",
    "!dist/**/{__mocks__,__tests__}/",
    "templates/*.hbs"
  ],
  "prepublish": "build",
  "dependencies": {
    "case": "1.6.3",
    "cli-color": "2.0.4",
    "commander": "13.0.0",
    "glob": "11.0.0",
    "handlebars": "4.7.8",
    "slugify": "1.6.6",
    "svg2ttf": "6.0.3",
    "svgicons2svgfont": "12.0.0",
    "ttf2eot": "3.1.0",
    "ttf2woff": "3.0.0",
    "ttf2woff2": "5.0.0"
  },
  "devDependencies": {
    "@types/cli-color": "2.0.6",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.5",
    "@types/svg2ttf": "5.0.3",
    "@types/ttf2eot": "2.0.2",
    "@types/ttf2woff": "2.0.4",
    "@types/ttf2woff2": "2.0.2",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.1",
    "ts-jest": "29.2.5",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "./src",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "./tsconfig.test.json"
        }
      ]
    }
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
